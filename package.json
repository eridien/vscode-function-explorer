{
  "name": "vscode-function-explorer",
  "displayName": "Function Explorer",
  "description": "Function explorer and marked functions.",
  "icon": "images/icon.png",
  "version": "0.1.6",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eridien/vscode-function-explorer"
  },
  "publisher": "eridien",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-function-explorer.toggle",
        "title": "Toggle Function Mark(s)",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-function-explorer.prev",
        "title": "Jump to previous Function Mark"
      },
      {
        "command": "vscode-function-explorer.next",
        "title": "Jump to next Function Mark"
      },
      {
        "command": "vscode-function-explorer.toggleMarkedFilter",
        "title": " ✔ ",
        "category": "Function Explorer: Toggle Mark Filter"
      },
      {
        "command": "vscode-function-explorer.toggleMarkedFilterMenu",
        "title": " Toggle Mark Filter"
      },
      {
        "command": "vscode-function-explorer.toggleAlphaSort",
        "title": " A ",
        "category": "Function Explorer: Toggle Alpha Sort"
      },
      {
        "command": "vscode-function-explorer.toggleAlphaSortMenu",
        "title": "Toggle Alpha Sort"
      },
      {
        "command": "vscode-function-explorer.toggleItemMark",
        "title": "M",
        "category": "Function Explorer: Toggle Function Mark"
      },
      {
        "command": "vscode-function-explorer.toggleItemMarkMenu",
        "title": "Toggle Function Mark"
      },
      {
        "command":  "vscode-function-explorer.removeMarks",
        "title":    " ⨉ ",
        "category": "Function Explorer: Remove Marks"
      },
      {
        "command":  "vscode-function-explorer.removeMarksMenu",
        "title":    "Remove Marks"
      },
      {
        "command":  "vscode-function-explorer.funcClickCmd",
        "title":    "Show the function in an editor.",
        "category": "Vscode-function-explorer"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-function-explorer.toggle",
        "key": "ctrl+alt+\\",
        "mac": "cmd+alt+\\",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-function-explorer.prev",
        "key": "ctrl+alt+[",
        "mac": "cmd+alt+["
      },
      {
        "command": "vscode-function-explorer.next",
        "key": "ctrl+alt+]",
        "mac": "cmd+alt+]"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "functionMarks",
          "title": "Function Explorer",
          "icon": "images/activity-icon.svg"
        }
      ]
    },
    "views": {
      "functionMarks": [
        {
          "id": "sidebarView",
          "name": "Function Explorer",
          "icon": "images/activity-icon.svg"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "vscode-function-explorer.toggle",
          "when": "editorTextFocus",
          "group": "1_modification"
        }
      ],
      "view/item/context": [
        {
          "command": "vscode-function-explorer.toggleMarkedFilter",
          "when": "view == sidebarView && viewItem == file",
          "group": "inline@1"
        },{
          "command": "vscode-function-explorer.toggleMarkedFilterMenu",
          "when": "view == sidebarView && viewItem == file"
        },{
          "command": "vscode-function-explorer.toggleAlphaSort",
          "when": "view == sidebarView && viewItem == file",
          "group": "inline@2"
        },{
          "command": "vscode-function-explorer.toggleAlphaSortMenu",
          "when": "view == sidebarView && viewItem == file"
        },{
          "command": "vscode-function-explorer.toggleItemMark",
          "when": "view == sidebarView && viewItem == func",
          "group": "inline@2"
        },{
          "command": "vscode-function-explorer.toggleItemMarkMenu",
          "when": "view == sidebarView && viewItem == func"
        },{
          "command": "vscode-function-explorer.removeMarks",
          "when": "view == sidebarView && viewItem == wsFolder",
          "group": "inline@3"
        },{
          "command": "vscode-function-explorer.removeMarksMenu",
          "when": "view == sidebarView && viewItem == wsFolder"
        },{
          "command": "vscode-function-explorer.removeMarks",
          "when": "view == sidebarView && viewItem == folder",
          "group": "inline@3"
        },{
          "command": "vscode-function-explorer.removeMarksMenu",
          "when": "view == sidebarView && viewItem == folder"
        },{
          "command": "vscode-function-explorer.removeMarks",
          "when": "view == sidebarView && viewItem == file",
          "group": "inline@4"
        },{
          "command": "vscode-function-explorer.removeMarksMenu",
          "when": "view == sidebarView && viewItem == file"
        }
      ]
    },
    "configuration": {
      "title": "Function Explorer",
      "properties": {
        "flattenFolders": {
          "type": "boolean",
          "default": true,
          "order": 0,
          "description": "Flatten folders in the function explorer. If disabled, all folders will be shown with their children which will make the functions further to the right and less visible."
        },
        "function-explorer.showFileOnFileOpen": {
          "type": "boolean",
          "default": true,
          "order": 1,
          "description": "When expanding a file in the explorer, show the file in an editor."
        },
        "function-explorer.fileWrap": {
          "type": "boolean",
          "default": true,
          "order": 2,
          "description": "Next and previous wrap around all files instead of just active file."
        },
        "function-explorer.scrollPosition": {
          "type": "string",
          "order": 3,
          "default": "At Top (with Margin)",
          "enum": [
            "Minimal Scrolling",
            "In Center",
            "In Center If Needed",
            "At Top (with Margin)"
          ],
          "enumDescriptions": [
            "Reveal with minimal scrolling",
            "Reveal in the center of the viewport",
            "Center only if not visible",
            "Reveal at the top of the viewport (with margin above function)"
          ],
          "description": "Controls the scroll position when revealing a function."
        },
        "function-explorer.topMargin": {
          "type": "number",
          "default": 3,
          "order": 4,
          "description": "Number of margin lines above function when scrolling with \"At Top (with Margin)\"."
        },
        "function-explorer.alphaSortFuncs": {
          "type": "boolean",
          "default": false,
          "order": 5,
          "description": "Sort functions in the file alphabetically instead of by position. This can be overidden in the sidebar file \"A\" button."
        },
        "function-explorer.filesToInclude": {
          "type": "string",
          "default": "**/*.js, **/*.ts",
          "order": 6,
          "description": "Files to include in the function explorer. Separate glob patterns with commas."
        },
        "function-explorer.filesToExclude": {
          "type": "string",
          "default": "node_modules/**",
          "order": 7,
          "description": "Files to exclude in the function explorer. Separate glob patterns with commas."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/babel__parser": "^7.0.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@babel/types": "^7.27.3",
    "acorn-loose": "^8.5.0",
    "acorn-walk": "^8.3.4",
    "minimatch": "^10.0.2",
    "typescript-parser": "^2.6.1"
  }
}
